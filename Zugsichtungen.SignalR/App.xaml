<appbase:AppBase x:Class="Zugsichtungen.SignalR.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Zugsichtungen.SignalR"
             xmlns:appbase="clr-namespace:Zugsichtungen.ApplicationBase;assembly=Zugsichtungen.ApplicationBase">
    <appbase:AppBase.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Zugsichtungen.Styles;component/Themes/Generic.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Zugsichtungen.SharedViews;component/Templates.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </appbase:AppBase.Resources>
</appbase:AppBase>
